[user]
	name = Username
	email = E-Mail
[alias]
	co = checkout
[color]
	diff = auto
	status = auto
	branch = auto
[core]
	excludesfile = $HOME.gitignore
	editor = subl --wait
	autocrlf = input
[apply]
	whitespace = nowarn
[format]
	pretty = %C(yellow)%h%Creset %s %C(red)(%an, %cr)%Creset
[github]
	user = GithubUser
	token = Token
[push]
	default = simple
